<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.seu.herald_android">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".debug.CrashApplication"
        android:allowBackup="false"
        tools:replace="android:allowBackup"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <!--启动页-->
        <activity
            android:name=".mod_communicate.WelcomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Invisible">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".app_main.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".mod_auth.LoginActivity"
            android:label="@string/title_activity_login"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".mod_modulemanager.ModuleManageActivity"
            android:label="@string/title_activity_query"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/SubActivityTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.schoolbus.SchoolBusActivity"
            android:label="@string/title_activity_schoolbus"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/QuerySchoolBusTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_SCHOOLBUS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.grade.GradeActivity"
            android:label="@string/title_activity_grade"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/QueryGradeTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_GRADE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mod_query.library.LibraryActivity"
            android:label="@string/title_activity_library"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/LibraryTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_LIBRARY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mod_query.schedule.SchoolScheduleActivity"
            android:label="@string/title_activity_schedule"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/ScheduleTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_SCHEDULE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.library.LibrarySearchActivity"
            android:label="@string/title_activity_library_search"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/LibraryTheme">
        </activity>

        <activity
            android:name=".mod_query.lecture.LectureActivity"
            android:label="@string/title_activity_lecture"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/LectureTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_LECTURE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.cardextra.CardActivity"
            android:label="@string/title_activity_card"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/CardTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_CARDEXTRA" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.experiment.ExperimentActivity"
            android:label="@string/title_activity_experiment"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/ExperimentTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_EXPERIMENT" />
                <category android:name="android.intent.category.DEFAULT" />
