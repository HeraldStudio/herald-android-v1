        <activity
            android:name=".mod_query.seunet.SeunetActivity"
            android:label="@string/title_activity_seunet"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/SeunetTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_SEUNET" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.gymreserve.GymReserveActivity"
            android:label="@string/title_activity_gymreserve"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/GymReserveTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_GYMRESERVE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_local.todo.TodoActivity"
            android:label="@string/title_activity_todo"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/TodoTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_LOCAL_TODO" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.pedetail.PedetailActivity"
            android:label="@string/title_activity_pedetail"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/PedetailTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_PEDETAIL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_query.curriculum.CurriculumActivity"
            android:label="@string/title_activity_curriculum"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/CurriculumTheme">
            <intent-filter>
                <action android:name="cn.seu.herald_android.MODULE_QUERY_CURRICULUM" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name=".mod_webmodule.WebModuleActivity"
            android:theme="@style/AppTheme.Invisible"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="cn.seu.herald_android.WEBMODULE_GYMORDER" />
                <action android:name="cn.seu.herald_android.WEBMODULE_EMPTYROOM" />
                <action android:name="cn.seu.herald_android.WEBMODULE_QUANYI" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".mod_communicate.AboutusActivity"
            android:label="@string/title_activity_aboutus"
            android:theme="@style/AboutusTheme"
            android:screenOrientation="portrait" />

        <activity
            android:name=".mod_communicate.FeedbackActivity"
            android:label="@string/title_activity_feedback"
            android:theme="@style/FeedbackTheme"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <activity
            android:name=".mod_query.srtp.SrtpActivity"
            android:label="@string/title_activity_srtp"
            android:theme="@style/SrtpTheme"
